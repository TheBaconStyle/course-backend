{
  "kind": "collectionType",
  "collectionName": "sessions",
  "info": {
    "singularName": "session",
    "pluralName": "sessions",
    "displayName": "Session"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::student.student",
      "inversedBy": "sessions"
    },
    "groups": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::group.group",
      "inversedBy": "sessions"
    },
    "test": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::test.test",
      "inversedBy": "sessions"
    },
    "attempts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attempt.attempt",
      "mappedBy": "session"
    },
    "start": {
      "type": "date"
    },
    "finish": {
      "type": "date"
    },
    "attempt_count": {
      "type": "integer"
    },
    "evaluation_method": {
      "type": "enumeration",
      "enum": [
        "best",
        "last",
        "avg"
      ]
    }
  }
}
